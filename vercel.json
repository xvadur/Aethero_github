{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/dashboard/(.*)",
      "dest": "dashboard/public/$1"
    },
    {
      "src": "/",
      "dest": "dashboard/public/index.html"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  },
  "public": true
}
